//ligne 27

class Math =
(
	method pow(a : int, b : int) : int {
		var retval : int;
		var i : int;

		if b == 0 then
			return (1);
		fi
		retval := 1;
		for i in 1 .. b
		do
			retval := retval * a;
		end

		return (retval);
	}

)


var m : Math;
var n : int
n:=3;
var res : Math;
m := new Math;
res := m.pow(n,n);